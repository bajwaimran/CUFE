
@{
    ViewBag.Title = "My Loads";
    Layout = "~/Views/Shared/_LayoutBackend.cshtml";
}

<script src="~/Scripts/LoadScripts.js"></script>
<script src="~/Scripts/GeoCodeScripts.js"></script>

@Html.Action("MyGridViewPartial", "Loads")


@Html.DevExpress().RoundPanel(settings => {
    settings.Name = "roundPanel";
    settings.Width = System.Web.UI.WebControls.Unit.Percentage(100);
    settings.HeaderText = "Load Details:";
    settings.SetContent(() => {
        Html.RenderPartial("LoadDetails");
    });
}).GetHtml()